<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.incquerylabs.evm.xtext.ui.fire"
            name="Fire EVM-Xtext Integration">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="com.incquerylabs.evm.xtext.ui.toolbar1">
            <command
                  commandId="com.incquerylabs.evm.xtext.ui.fire"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.incquerylabs.evm.xtext.ui.handlers.FireXtextListener"
            commandId="com.incquerylabs.evm.xtext.ui.fire">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.incquerylabs.evm.xtext.ui.util.XtextIntegrationExecutableExtensionFactory:com.incquerylabs.evm.xtext.ui.views.XtendClassesView"
            id="com.incquerylabs.evm.xtext.ui.inferredclasses"
            name="JVM Types Demo"
            restorable="true">
      </view>
      <view
            class="com.incquerylabs.evm.xtext.ui.util.XtextIntegrationExecutableExtensionFactory:com.incquerylabs.evm.xtext.ui.views.InferredClassViewerView"
            id="com.incquerylabs.evm.xtext.ui.inferredclasses.viewer"
            name="JVM Types Viewer Demo"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.console.consoleFactories">
      <consoleFactory
            class="com.incquerylabs.evm.xtext.ui.util.XtextIntegrationExecutableExtensionFactory:com.incquerylabs.evm.xtext.ui.XtextIndexConsoleFactory"
            label="Xtext Index Console">
      </consoleFactory>
   </extension>
</plugin>
